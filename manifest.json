{
    "manifest_version": 3,
    "name": "CRXJS Vanilla JS Example",
    "version": "1.0.0",
    "permissions": ["scripting", "webNavigation", "storage"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "js": [
          "./src/content.ts"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
